		"husky": "^7.0.4",
		"mdsvex": "^0.9.8",
		"rehype-autolink-headings": "^6.1.0",
		"rehype-slug": "^5.0.0",
		"remark-parse": "^10.0.0",
		"svelte": "^3.44.1",
		"svelte-preprocess": "^4.0.0",
		"tslib": "^2.2.0",
		"typescript": "^4.0.0",
		"unified": "^10.1.0"
	},
	"type": "module",
	"husky": {
		"hooks": {
			"pre-commit": "yarn cache-search && git add src/cache/data.js"
		}
	},
	"dependencies": {
		"fuse.js": "^6.4.6"
	}
